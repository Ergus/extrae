<clustering_definition use_duration="no" apply_log="yes" normalize_data="yes" duration_filter="2500" threshold_filter="0">
  <DBSCAN epsilon=".015" min_points="10"/>
  <clustering_parameters>
    <single_event apply_log="yes" name="Native_Instr">
      <event_type>42001090</event_type>
      <factor>1.0</factor>
    </single_event>

    <mixed_events apply_log="no" name="cl_IPC" operation="/">
      <event_type_a>42001090</event_type_a>
      <event_type_b>42001008</event_type_b>
      <factor>1.0</factor>
    </mixed_events>

  </clustering_parameters>

  <projection_parameters>

    <single_event apply_log="yes" name="PM_INST_DISP">
      <event_type>42001091</event_type>
      <factor>1.0</factor>
    </single_event>

    <single_event apply_log="yes" name="PM_DATA_FROM_MEM">
      <event_type>42001012</event_type>
      <factor>1.0</factor>
    </single_event>

    <single_event apply_log="yes" name="PM_LD_MISS_L1">
      <event_type>42001105</event_type>
      <factor>1.0</factor>
    </single_event>

    <single_event apply_log="yes" name="PM_CYC">
      <event_type>42001008</event_type>
      <factor>1.0</factor>
    </single_event>

    <single_event apply_log="yes" name="PM_GCT_FULL_CYC">
      <event_type>42001075</event_type>
      <factor>1.0</factor>
    </single_event>

    <single_event apply_log="yes" name="PM_INST_CMPL">
      <event_type>42001090</event_type>
      <factor>1.0</factor>
    </single_event>

    <single_event apply_log="yes" name="PM_ST_REF_L1">
      <event_type>42001207</event_type>
      <factor>1.0</factor>
    </single_event>

    <single_event apply_log="yes" name="PM_LD_REF_L1">
      <event_type>42001108</event_type>
      <factor>1.0</factor>
    </single_event>

  </projection_parameters>

  <plot_options raw_metrics="yes">
    <x_metric title="IPC">cl_IPC</x_metric>
    <y_metric title="Instructions Completed">Native_Instr</y_metric>
  </plot_options>
</clustering_definition>
